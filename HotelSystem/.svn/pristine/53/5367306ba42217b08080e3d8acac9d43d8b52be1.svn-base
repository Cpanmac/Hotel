	text-decoration: none;
}
.top {
	background: rgb(127, 31, 89);/* 深颜色 */
	height: 4px;
}

.logo_bar {
	background: rgb(244, 239, 242);
	height: 90px;
}

.logo_bar img {
	width: 120px;
	position: absolute;
	left: 40px;
	top: 20px;
}
.logo_bar span{
	float: right;
	line-height: 90px;
	margin-right: 60px;
}
.logo_bar span a{
	color:rgb(127, 31, 89);
