input,textarea {
	border: 1px solid #ddeafb;
}

input[type=submit],input[type=button] {
	cursor: pointer;
}
input #btnSendCode{
	color:red;
	font-size: 19px;
	width: 155px;
	color:rgb(127, 31, 89);
}
input {
	 width: 100%; 
	box-sizing: border-box;
	padding: 10px;
	outline: none;
	margin-bottom: 2px;
	border-radius: 5px;
	/* background-color: pink; */
}



.hint {
	font-size: 12px;
	text-align: right;
	padding-right: 10px;
	margin-bottom: 15px;
	visibility: hidden;
	/* background-color: red; */
	height: 10px;
}

.right {
	border-color: #1dc091;
}

.hint_right {
	color: #1dc091;
}

.wrong {
	border-color: #EA5A70;
}

.hint_wrong {
	color: #EA5A70;
}

input[type=submit],.button {
	display: block;
	margin: 0 auto;
	line-height: 20px;
	font-size: 20px;
	color: #fff;
	background: rgb(127, 31, 89);
	border: 0;
}

.lable {
	display: inline-block;
}

